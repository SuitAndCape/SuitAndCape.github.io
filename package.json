{
  "title": "RAPTORFrame",
  "name": "raptorframe",
  "version": "1.0.3",
  "description": "",
  "scripts": {
    "raptor": "gulp",
    "lint-js": "eslint --ignore-pattern ./frame/**/*,./source/javascripts/vendors/**/* ./source/javascripts/**/*.js || exit 0",
    "launch": "http-server -o"
  },
  "homepage": "https://github.com/SuitAndCape/RAPTORFrame",
  "repository": {
    "type": "git",
    "url": "https://github.com/SuitAndCape/RAPTORFrame"
  },
  "bugs": {
    "url": "https://github.com/SuitAndCape/RAPTORFrame/issues"
  },
  "author": {
    "name": "Ali Esmaili",
    "email": "AESM85@gmail.com",
    "url": "http://SuitAndCape.github.io"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/SuitAndCape/RAPTORFrame/blob/Info/LICENSE"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-standard": "^1.3.1",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-sass-glob": "^0.0.8",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "http-server": "^0.8.5"
  },
  "private": true,
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "no-extra-semi": 2,
      "semi":
      [
        2,
        "always"
      ],
      "spaced-comment":
      [
        2,
        "always", {
          "exceptions":
          [
            "============================================================================//"
          ],
          "markers":
          [
            "/",
            "//=",
            "=="
          ]
        }
      ]
    }
  },
  "keywords":
  [
    "RAPTORSMACSS",
    "SCSS",
    "Sass",
    "SMACSS",
    "CSS",
    "HTML",
    "JavaScript",
    "Gulp",
    "framework",
    "frontend",
    "front-end",
    "front end",
    "SuitAndCape"
  ]
}
